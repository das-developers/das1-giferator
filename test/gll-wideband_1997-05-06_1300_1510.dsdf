description	= 'Galileo PWS HRS LPW 10-kHz relative power spectrum'
form		= x_tagged_y_scan ; time tag and frequency spectrum
reader		= 'cat ../test/gll-wideband_1997-05-06_1300_1510.dat && echo'
spacecraft	= 'Galileo'
spacecraft_id	= 'GO'
instrument	= 'Plasma Wave Subsystem'
instrument1	= 'Wideband'
instrument_id	= 'PWS'
instrument_id1  = 'HRS'
x_parameter	= 'time'
x_unit		= 's'
x_sample_width	= 15.0 ; seconds between spectra
y_parameter	= 'frequency'
y_unit		= 'Hz'
y_coordinate	= findgen(433) / 432.0 * 12600.0
z_parameter	= 'amplitude'
z_unit		= 'relative power'
z_fill		= 1.0e-32 ; substitute this in place of missing data
